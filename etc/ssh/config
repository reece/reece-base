# FORCED DEFAULTS
ForwardAgent yes
ConnectionAttempts 3
HashKnownHosts no
StrictHostKeyChecking no
ForwardX11 yes
IdentityFile ~/.ssh/id
ControlPath ~/.ssh/controlmaster-%r@%h:%p


Host whoville-localhost
	 HostName 127.0.0.1
	 HostKeyAlias whoville
Host whoville-internal whoville w
	 HostName 192.168.0.175
	 HostKeyAlias whoville
Host whoville-external
	 HostName whoville.homelinux.net
	 HostKeyAlias whoville

Host hn unison-relay unison-relay-wget
	 HostName harts.net
	 User hartsnet

Host bitbucket.org
	 IdentityFile ~/.ssh/bitbucket

Host sf
	 HostName shell.sf.net
	 User reece


